            <li>
                <a href="http://www.linkedin.com/shareArticle?mini=true&url=@Url.Action("Open", null, new { Id = Model.Preek.Id }, Request.Url.Scheme)&title=Preek op prekenweb.nl" class="" title="" id="deelOpLinkedinLink" target="_blank">
