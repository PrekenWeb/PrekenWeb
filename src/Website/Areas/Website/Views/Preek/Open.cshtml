                    <a href="#" onclick="prekenweb.toonMijnReclame('Bladwijzer'); return false;" title="@Resources.BladwijzerTooltip" class="tooltip">
                        <div class="fa fa-bookmark"></div>
                        @Resources.BladwijzerLeggen
