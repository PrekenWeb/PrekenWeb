            <h2>@Resources.LeesPreekTekst</h2>
            @if (!string.IsNullOrWhiteSpace(Model.Preek.LeesPreekTekst) && Model.Preek.PreekTypeId == (int)PreekTypeEnum.LeesPreek)
            {
                <div class="HeleLeespreek">
                    <table class="HeleLeespreekTable">
