                <a href="http://twitter.com/share?url=@Url.Action("Open", null, new { Id = Model.Preek.Id }, Request.Url.Scheme)" class="" id="deelOpTwitterLink" title="" target="_blank">
                    <div class="fa fa-twitter-square"></div>
