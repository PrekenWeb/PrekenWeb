            </div>
            @if (Model.Preek.DatumAangemaakt.HasValue && Model.Preek.DatumBijgewerkt.HasValue && (Model.Preek.DatumAangemaakt.Value - Model.Preek.DatumBijgewerkt.Value).TotalDays > 1)
